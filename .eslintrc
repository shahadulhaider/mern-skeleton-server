{
  "extends": ["airbnb","prettier"],
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ]
  }
}
